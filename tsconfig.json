{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "traceResolution": false,
    "sourceMap": false,
    "outDir": "./dist",
    "rootDir": "./src",
    "importHelpers": true,
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
